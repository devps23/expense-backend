pipeline {
//     agent any
    agent { node {label 'ci-server'}}
    stages {
         stage('Lint code') {
            steps{
                sh 'env'
                echo 'Hello World'
                 }
            }
         stage('Run unit tests') {
            steps{
                echo 'Hello World'
                 }
            }
         stage('Run Integration tests') {
            steps{
                echo 'Hello World'
                }
            }
         stage('Sonar Scan Code Review'){
            steps{
                echo 'Hello World'
                }
            }
         stage('npm install'){
            steps{
                echo 'Hello World'
                }
             }
         stage('Release Software'){
            steps{
                echo 'Hello World'
                 }
            }
    }
}

